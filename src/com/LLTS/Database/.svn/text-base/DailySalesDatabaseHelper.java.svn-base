package com.LLTS.Database;

public class DailySalesDatabaseHelper {
	
	public static String transactionID = "TransactionID";
	public static String packID = "PackID";
	public static String openTicketNumber = "OpenTicketNumber";
	public static String closeTicketNumber = "CloseTicketNumber";
	public static String shiftInfo = "ShiftInformation";
	public static String shiftOpenDate = "ShiftOpenDate";
	public static String shiftCloseDate = "ShiftCloseDate";
	
	int transactionIDValue;	
	int packIDValue;
	int openTicketNumberValue;
	int closeTicketNumberValue;
	long openTicketDateValue;
	long closeTicketDateValue;
	
	
	
	public int getTransactionIDValue() {
		return transactionIDValue;
	}

	public void setTransactionIDValue(int transactionIDValue) {
		this.transactionIDValue = transactionIDValue;
	}
	public long getOpenTicketDateValue() {
		return openTicketDateValue;
	}

	public void setOpenTicketDateValue(long openTicketDateValue) {
		this.openTicketDateValue = openTicketDateValue;
	}

	public long getCloseTicketDateValue() {
		return closeTicketDateValue;
	}

	public void setCloseTicketDateValue(long closeTicketDateValue) {
		this.closeTicketDateValue = closeTicketDateValue;
	}
	String shiftInfoValue;
	
	public int getTicketSold() {
		return closeTicketNumberValue-openTicketNumberValue;
	}
	
	public int getPackIDValue() {
		return packIDValue;
	}
	public void setPackIDValue(int packIDValue) {
		this.packIDValue = packIDValue;
	}
	public int getOpenTicketNumberValue() {
		return openTicketNumberValue;
	}
	public void setOpenTicketNumberValue(int openTicketNumberValue) {
		this.openTicketNumberValue = openTicketNumberValue;
	}
	public int getCloseTicketNumberValue() {
		return closeTicketNumberValue;
	}
	public void setCloseTicketNumberValue(int closeTicketNumberValue) {
		this.closeTicketNumberValue = closeTicketNumberValue;
	}
	public String getShiftInfoValue() {
		return shiftInfoValue;
	}
	public void setShiftInfoValue(String shiftInfoValue) {
		this.shiftInfoValue = shiftInfoValue;
	}

}
